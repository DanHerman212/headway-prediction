# GCP Configuration
GCP_PROJECT_ID=your-project
VERTEX_LOCATION=us-east1

# Data Extraction
TRACK=A1
ROUTE_IDS=A,C,E

# Data Splits
TRAIN_SPLIT=0.6
VAL_SPLIT=0.2
TEST_SPLIT=0.2

# Model Architecture
MODEL_NAME=stacked_gru_headway_predictor
MODEL_TYPE=stacked_gru
GRU_UNITS=128,64
DROPOUT_RATE=0.2
RECURRENT_DROPOUT=0.1

# Training
LOOKBACK_STEPS=20
BATCH_SIZE=64
EPOCHS=50
LEARNING_RATE=0.001
OPTIMIZER=adam

# Loss Functions & Metrics
REGRESSION_LOSS=huber
CLASSIFICATION_LOSS=sparse_categorical_crossentropy
HUBER_DELTA=1.0
LOSS_WEIGHTS_HEADWAY=1.0
LOSS_WEIGHTS_ROUTE=0.5

# Experiment Tracking
EXPERIMENT_NAME=your-experiment
USE_VERTEX_EXPERIMENTS=true
TRACK_HISTOGRAMS=true
HISTOGRAM_FREQ=5
TRACK_PROFILING=false
PROFILE_BATCH_START=10
PROFILE_BATCH_END=20
